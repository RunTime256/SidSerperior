<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sql.mapper.ServerMapper">
    <select id="getServerId" resultType="Long">
        SELECT server_id
        FROM bot.server
        WHERE name = #{serverName}
    </select>

    <select id="getAllServerIds" resultType="Long">
        SELECT server_id
        FROM bot.server
    </select>
</mapper>
